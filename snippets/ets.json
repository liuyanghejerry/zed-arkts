{
  "ETS Class": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName} {",
      "  ${2:// constructor}",
      "  constructor() {",
      "    ${3}",
      "  }",
      "  ",
      "  ${4:// methods}",
      "  ${5:method}() {",
      "    ${6}",
      "  }",
      "}"
    ],
    "description": "Create a new ETS class"
  },
  "ETS Interface": {
    "prefix": "interface",
    "body": [
      "interface ${1:InterfaceName} {",
      "  ${2:property}: ${3:string};",
      "  ${4:method}(): ${5:void};",
      "}"
    ],
    "description": "Create a new ETS interface"
  },
  "ETS Function": {
    "prefix": "function",
    "body": [
      "function ${1:functionName}(${2:params}): ${3:returnType} {",
      "  ${4}",
      "}"
    ],
    "description": "Create a new ETS function"
  },
  "ETS Arrow Function": {
    "prefix": "arrow",
    "body": [
      "const ${1:functionName} = (${2:params}): ${3:returnType} => {",
      "  ${4}",
      "};"
    ],
    "description": "Create a new ETS arrow function"
  },
  "ETS Import": {
    "prefix": "import",
    "body": [
      "import { ${1:imports} } from '${2:module}';"
    ],
    "description": "Import from a module"
  },
  "ETS Export Default": {
    "prefix": "export",
    "body": [
      "export default ${1:exportName};"
    ],
    "description": "Export default"
  },
  "ETS Console Log": {
    "prefix": "log",
    "body": [
      "console.log(${1:message});"
    ],
    "description": "Console log statement"
  }
}
